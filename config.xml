<?xml version='1.0' encoding='utf-8' ?>
<widget id="com.lmsoluciones.gemaapp" 
		version="1.0.5" 
		xmlns="http://www.w3.org/ns/widgets" 
		xmlns:gap="https://phonegap.com/ns/1.0" 
		xmlns:cdv="http://cordova.apache.org/ns/1.0">

<name>Gema App</name>

<description>
    Gema App, es una aplicacion creada para los propietarios de buses urbanos de empresas con el Sistema de Gestion GEMA, En esta app podras ver informacion en tiempo real de las ventas, recorridos, alarmas e informacion general de todos tus buses.
</description>

<author email="soporte@lmsoluciones.co" href="http://www.lmsoluciones.co">
    LM SOLUCIONES
</author>

<content src="index.html" />
<access origin="*" subdomains="true" />

<allow-intent target="_blank" href="http://*/*" />
<allow-intent target="_blank" href="https://*/*" />
<allow-intent target="_blank" href="tel:*" />
<allow-intent target="_blank" href="sms:*" />
<allow-intent target="_blank" href="mailto:*" />
<allow-intent target="_blank" href="geo:*" />
<platform name="android">
    <allow-intent href="market:*" />
    <plugin name="cordova-android-support-gradle-release" source="npm">
        <variable name="ANDROID_SUPPORT_VERSION" value="28.0.0"/>
    </plugin>
</platform>
<platform name="ios">
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
</platform>

<plugin name="cordova-plugin-whitelist" source="npm"/>
<plugin name="cordova-plugin-googlemaps" source="npm"/>
<plugin name="cordova-plugin-browsersync" source="npm"/>

<preference name="phonegap-version" value="cli-9.0.0" />
<preference name="GOOGLE_MAPS_ANDROID_API_KEY" value="AIzaSyBvrBF3dUzhRUxM4C3Zh_OMAms0-gigDOU" />
<preference name="GOOGLE_MAPS_IOS_API_KEY" value="AIzaSyBvrBF3dUzhRUxM4C3Zh_OMAms0-gigDOU" />

<engine name="ios" spec="^5.0.1" />
<engine name="android" spec="^8.0.0" />
<engine name="browser" spec="~5.0.4" />

<icon src="images/icon.png" />

</widget>